meta {
  name: CreateUser
  type: http
  seq: 1
}

post {
  url: {{api_url}}/tenant/users
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
  Accept: application/json
  X-Tenant-ID: {{tenant_id}}
}

body:json {
  {
    "email": "{{user_email}}",
    "password": "{{user_password}}"
  }
}

vars:pre-request {
  user_email: user1@acme.local
  user_password: Changeme@1acme
}

tests {
  test("Status is 200", function() {
    expect(res.status).to.equal(200);
  });
}
