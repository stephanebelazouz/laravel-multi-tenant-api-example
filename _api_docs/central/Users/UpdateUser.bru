meta {
  name: UpdateUser
  type: http
  seq: 1
}

post {
  url: {{api_url}}/users/{{user_id}}
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
  Accept: application/json
}

body:json {
  {
    "email": {{user_email}},
    "password": {{user_password}}
  }
}

tests {
  test("Status is 200", function() {
    expect(res.status).to.equal(200);
  });
}
